    "jbuild": "node node_modules/.bin/webpack --config config/webpack/development.js",
    "build:server": "./node_modules/.bin/webpack --config config/webpack/server.js",
    "slate-server": "yarn build:server && node --max-old-space-size=2048 script/slate-server.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.js.erb app/frontend app/javascripts app/assets/javascripts app/views spec/frontend lib/eln",
