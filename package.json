  "scripts": {
    "start": "./bin/webpack-dev-server",
    "jstart": "node node_modules/.bin/webpack-dev-server --config config/webpack/development.js",
    "jstartX": "node node_modules/.bin/webpack serve --config config/webpack/development.js",
    "jstartY": "node node_modules/.bin/webpack serve --config config/webpack/development.js",
    "build": "./bin/webpack",
    "jbuild": "node node_modules/.bin/webpack --config config/webpack/development.js",
    "build:server": "./node_modules/.bin/webpack --config config/webpack/server.js",
    "slate-server": "yarn build:server && node --max-old-space-size=2048 script/slate-server.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.js.erb app/frontend app/javascripts app/assets/javascripts app/views spec/frontend lib/eln",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js,.jsx,.js.erb app/frontend app/javascripts app/assets/javascripts app/views spec/frontend lib/eln",
    "lint:sass": "sass-lint -q -v",
    "lint:security": "./node_modules/.bin/eslint --ext .js,.js.erb,.jsx -c .eslintrc.security.json app/assets/javascripts app/frontend app/javascripts app/views",
    "flow": "cd app/frontend && ../../node_modules/.bin/flow",
    "test": "yarn test:browser && yarn test:node",
    "test:browser": "NODE_OPTIONS='--max-old-space-size=9216' ./node_modules/.bin/karma start config/karma.config.js",
    "test:node": "NODE_OPTIONS='--max-old-space-size=9216' node --inspect=0.0.0.0 ./node_modules/.bin/mocha-webpack --colors=false --webpack-config config/webpack/server.test.js --require lib/eln/modules/jsdom-global.js --require spec/lib/eln/index.js spec/lib/eln/**/*.spec.js",
    "test:nodeX": "NODE_OPTIONS='--max-old-space-size=9216' node --inspect=0.0.0.0 ./node_modules/.bin/mocha-webpack --colors=false --webpack-config config/webpack/server.test.js --require lib/eln/modules/jsdom-global.js --require spec/lib/eln/index.js spec/frontend/**/*spec.js",
    "watch:test:browser": "yarn test:browser --watch",
    "watch:test:node": "yarn test:node --watch"
