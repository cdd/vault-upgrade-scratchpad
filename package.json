    "lint:sass": "sass-lint -q -v",
    "lint:security": "./node_modules/.bin/eslint --ext .js,.js.erb,.jsx -c .eslintrc.security.json app/assets/javascripts app/frontend app/javascripts app/views",
    "flow": "cd app/frontend && ../../node_modules/.bin/flow",
    "test": "yarn test:browser && yarn test:node",
    "test:browser": "NODE_OPTIONS='--max-old-space-size=9216' ./node_modules/.bin/karma start config/karma.config.js",
    "test:node": "NODE_OPTIONS='--max-old-space-size=9216' node --inspect=0.0.0.0 ./node_modules/.bin/mocha-webpack --colors=false --webpack-config config/webpack/server.test.js --require lib/eln/modules/jsdom-global.js --require spec/lib/eln/index.js spec/lib/eln/**/*.spec.js",
    "test:nodeX": "NODE_OPTIONS='--max-old-space-size=9216' node --inspect=0.0.0.0 ./node_modules/.bin/mocha-webpack --colors=false --webpack-config config/webpack/server.test.js --require lib/eln/modules/jsdom-global.js --require spec/lib/eln/index.js spec/frontend/**/*spec.js",
